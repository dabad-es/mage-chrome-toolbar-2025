{
  "manifest_version": 3,
  "name": "MageSpecialist DevTools for Magento",
  "description": "This extension can be used with MSP_DevTools module for Magento (Compatibilty with modern Chrome versions by David Abad)",
  "version": "3.3.4",
  "icons": {
    "16": "images/icon16x16.png",
    "48": "images/icon48x48.png",
    "128": "images/icon128x128.png"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "js/background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "js/jquery.js",
        "js/content.js"
      ]
    }
  ],
  "devtools_page": "devtools.html",
  "action": {
    "default_icon": "images/icon_off.png",
    "default_title": "Magento DevTools by MageSpecialist",
    "default_popup": "popup.html"
  },
  "content_security_policy": {},
  "host_permissions": [
    "*://*/*"
  ]
}